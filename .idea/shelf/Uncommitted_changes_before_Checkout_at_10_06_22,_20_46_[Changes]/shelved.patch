+ * @property integer $updated_at
+ * @property string $password write-only password
+ */
+class Admin extends ActiveRecord implements IdentityInterface
+{
+    const STATUS_DELETED = 0;
+    const STATUS_INACTIVE = 9;
+    const STATUS_ACTIVE = 10;
+
+
